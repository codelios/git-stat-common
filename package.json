{
    "name": "git-stat-common",
    "version": "0.7.62",
    "license": "MIT",
    "description": "Common Library code to refer git history",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/git-stat-common.git"
    },
    "keywords": [
        "git history",
        "vsce",
        "vsce plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "npm run prod",
        "clean": "rm -fr lib",
        "compile": "npm run clean && tsc",
        "dev": "rm -fr node_modules/fsevents && npm run compile && webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && npm run compile && webpack --mode production",
        "test": "npm run dev && mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts"
    },
    "dependencies": {
        "nodegit": "0.26.5"
    },
    "devDependencies": {
        "@types/node": "13.9.3",
        "ts-node": "8.8.1",
        "mocha": "7.1.1",
        "@types/mocha": "7.0.2",
        "chai": "4.2.0",
        "@types/chai": "4.2.11",
        "webpack": "4.42.1",
        "webpack-cli": "3.3.11",
        "ts-loader": "6.2.2",
        "minimist": ">=1.2.3"
    }
}
